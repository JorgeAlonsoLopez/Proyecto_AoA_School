<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.detalle.admin.detalleAeronave.DetalleAeronaveActivity">

    <ImageView
        android:id="@+id/img_foto_detalle_aeronave_admin"
        android:layout_width="310dp"
        android:layout_height="173dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="27dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="40dp"
        android:text="@string/text_matricula"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_foto_detalle_aeronave_admin" />

    <TextView
        android:id="@+id/text_matricula_detalle_aeronave_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:layout_marginStart="8dp"
        android:text="EC-000"
        app:layout_constraintBottom_toBottomOf="@+id/textView3"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/textView3"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/text_marca_detalle_aeronave_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Marca"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/text_modelo_detalle_aeronave_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:text="Modelo"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/text_marca_detalle_aeronave_admin"
        app:layout_constraintStart_toEndOf="@+id/text_marca_detalle_aeronave_admin"
        app:layout_constraintTop_toTopOf="@+id/text_marca_detalle_aeronave_admin"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/text_motor_detalle_aeronave_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Motor"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/text_marca_detalle_aeronave_admin"
        app:layout_constraintTop_toBottomOf="@+id/text_marca_detalle_aeronave_admin" />

    <TextView
        android:id="@+id/text_potencia_detalle_aeronave_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:layout_marginStart="96dp"
        android:text="0"
        app:layout_constraintBottom_toBottomOf="@+id/text_motor_detalle_aeronave_admin"
        app:layout_constraintStart_toEndOf="@+id/text_motor_detalle_aeronave_admin"
        app:layout_constraintTop_toTopOf="@+id/text_motor_detalle_aeronave_admin"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:textSize="18sp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="@string/hp"
        app:layout_constraintBottom_toBottomOf="@+id/text_potencia_detalle_aeronave_admin"
        app:layout_constraintStart_toEndOf="@+id/text_potencia_detalle_aeronave_admin"
        app:layout_constraintTop_toTopOf="@+id/text_potencia_detalle_aeronave_admin"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/autonomia"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/text_motor_detalle_aeronave_admin"
        app:layout_constraintTop_toBottomOf="@+id/text_motor_detalle_aeronave_admin" />

    <TextView
        android:id="@+id/text_autonomia_detalle_aeronave_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:layout_marginStart="10dp"
        android:text="0"
        app:layout_constraintBottom_toBottomOf="@+id/textView12"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toTopOf="@+id/textView12"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/VelMax"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/text_maxima_detalle_aeronave_admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:layout_marginStart="10dp"
        android:text="0"
        app:layout_constraintBottom_toBottomOf="@+id/textView14"
        app:layout_constraintStart_toEndOf="@+id/textView14"
