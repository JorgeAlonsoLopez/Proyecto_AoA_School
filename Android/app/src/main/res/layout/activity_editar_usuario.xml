<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D8D8D8"
    tools:context=".ui.detalle.admin.editarUsuario.EditarUsuarioActivity">

    <TextView
        android:id="@+id/text_tarj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/text_tarjeta"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/textView26"
        app:layout_constraintHorizontal_bias="0.457"
        app:layout_constraintStart_toStartOf="@+id/textView26"
        app:layout_constraintTop_toBottomOf="@+id/input_editar_usuario_fecha" />

    <EditText
        android:id="@+id/input_editar_tarjeta"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="@+id/text_tarj"
        app:layout_constraintHorizontal_bias="0.541"
        app:layout_constraintStart_toStartOf="@+id/text_tarj"
        app:layout_constraintTop_toBottomOf="@+id/text_tarj" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="@string/text_nombre_completo"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/text_email"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/input_editar_usuario_nombre"
        app:layout_constraintStart_toStartOf="@+id/input_editar_usuario_nombre"
        app:layout_constraintTop_toBottomOf="@+id/input_editar_usuario_nombre" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/text_telefono_sin"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/input_editar_usuario_correo"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/input_editar_usuario_correo"
        app:layout_constraintTop_toBottomOf="@+id/input_editar_usuario_correo" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/text_fechaNacimiento_son"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/input_editar_usuario_telef"
        app:layout_constraintStart_toStartOf="@+id/input_editar_usuario_telef"
        app:layout_constraintTop_toBottomOf="@+id/input_editar_usuario_telef" />

    <Button
        android:id="@+id/btn_confirm_editar_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="48dp"
        android:text="@string/text_confirm"
        app:cornerRadius="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/input_editar_tarjeta" />

    <Button
        android:id="@+id/btn_cancel_editar_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:text="@string/text_cancelar"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_confirm_editar_usuario"
        app:layout_constraintStart_toEndOf="@+id/btn_confirm_editar_usuario"
        app:layout_constraintTop_toTopOf="@+id/btn_confirm_editar_usuario"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/input_editar_usuario_nombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/textView21"
        app:layout_constraintStart_toStartOf="@+id/textView21"
        app:layout_constraintTop_toBottomOf="@+id/textView21" />

    <EditText
        android:id="@+id/input_editar_usuario_correo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="@+id/textView24"
        app:layout_constraintStart_toStartOf="@+id/textView24"
        app:layout_constraintTop_toBottomOf="@+id/textView24" />

    <EditText
        android:id="@+id/input_editar_usuario_telef"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="@+id/textView25"
        app:layout_constraintStart_toStartOf="@+id/textView25"
        app:layout_constraintTop_toBottomOf="@+id/textView25" />

    <EditText
        android:id="@+id/input_editar_usuario_fecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="false"
        android:ems="10"
        android:focusable="false"
        app:layout_constraintEnd_toEndOf="@+id/textView26"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="@+id/textView26"
        app:layout_constraintTop_toBottomOf="@+id/textView26" />
</androidx.constraintlayout.widget.ConstraintLayout>