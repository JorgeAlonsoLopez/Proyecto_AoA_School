<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="25dp"
    android:layout_marginBottom="15dp"
    android:orientation="horizontal">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="360dp"
        android:layout_height="180dp">


        <TextView
            android:id="@+id/text_inicio_lista_registro_admin_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="xx:xx"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/text_fecha2"
            app:layout_constraintStart_toEndOf="@+id/text_fecha2"
            app:layout_constraintTop_toTopOf="@+id/text_fecha2"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/text_fin_lista_registro_admin_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="xx:xx"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/text_fecha3"
            app:layout_constraintStart_toEndOf="@+id/text_fecha3"
            app:layout_constraintTop_toTopOf="@+id/text_fecha3"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/text_fecha2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="100dp"
            android:text="@string/text_hora_inicio"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_fecha3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="196dp"
            android:text="@string/text_horaFin"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/text_fecha2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/text_fecha2" />

        <TextView
            android:id="@+id/text_tipo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="128dp"
            android:text="@string/text_tipo"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/text_fecha"
            app:layout_constraintStart_toEndOf="@+id/text_fecha"
            app:layout_constraintTop_toTopOf="@+id/text_fecha"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/text_fecha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="@string/text_fecha"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginBottom="16dp"
            android:text="@string/text_aeronave"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/Text_registro_horas_matricula_admin_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="EC-000"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toTopOf="@+id/textView2" />

        <TextView
            android:id="@+id/text_fecha_lista_registro_admin_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:text="x/x/x"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/text_fecha"
            app:layout_constraintStart_toEndOf="@+id/text_fecha"
            app:layout_constraintTop_toTopOf="@+id/text_fecha"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/text_tipo_lista_registro_admin_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="x"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/text_tipo"
            app:layout_constraintStart_toEndOf="@+id/text_tipo"
            app:layout_constraintTop_toTopOf="@+id/text_tipo"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            android:text="@string/text_piloto"
            app:layout_constraintStart_toStartOf="@+id/text_fecha"
            app:layout_constraintTop_toBottomOf="@+id/text_fecha" />

        <TextView
            android:id="@+id/text_piloto_registro_admin_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:text="X"
            app:layout_constraintBottom_toBottomOf="@+id/textView7"
            app:layout_constraintStart_toEndOf="@+id/textView7"
            app:layout_constraintTop_toTopOf="@+id/textView7"
            app:layout_constraintVertical_bias="0.0" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</LinearLayout>